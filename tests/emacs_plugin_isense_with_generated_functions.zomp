/// 
/// Checking whether the Emacs plugin will display intellisense like help for
/// functions which are generated by macros
/// 

// requireLibs libcee

macro createFunctions:
  ret $:
    func void functionA():
    end
    func void functionB(int a, int b):
      printInt a
      printString ", "
      printInt b
      printNewline()
    end
    func void functionC(float f):
    end
  end
end

macro createFunctions2:
  ret $:
    func void functionX():
    end
    func void functionY(int a, int b):
    end
    func void functionZ(float f):
    end
  end
end

createFunctions

func int main():
  functionA()
  functionB(1,2)
  functionC(3.0f)
  // printString "foo\n"
  // printString "bar"
  // printString "baz"
  ret 0
end


