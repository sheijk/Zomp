/**
 * Calculating fibonacci numbers the sane way
 */

template op:= name value:
  var2 #name #value
end

func int fib(int n):
  prevprev := 1
  prev := 1

  for i 2 n:
    next := prev + prevprev
    prevprev = prev
    prev = next
  end

  ret prev
end

func int main():
  printlnString "Fibonaccis without the waiting"

  for i 1 47:
    printString "fib("
    printInt i
    printString ") = "
    printInt fib(i)
    printNewline()
  end

  ret 0
end

